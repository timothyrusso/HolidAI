{
  "expo": {
    "name": "HolidAI",
    "slug": "holidai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./ui/assets/images/icon.png",
    "scheme": "holidai",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./ui/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#220059"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.travelplanner",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.app.travelplanner",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./ui/assets/images/android-icon.png",
        "backgroundColor": "#220059"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./ui/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "react-native-map-link",
      "expo-font",
      "expo-build-properties",
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#220059",
          "image": "./ui/assets/images/icon.png",
          "dark": {
            "image": "./ui/assets/images/icon.png",
            "backgroundColor": "#220059"
          },
          "ios": {
            "imageWidth": 150,
            "resizeMode": "contain"
          },
          "android": {
            "imageWidth": 150
          }
        }
      ]
    ],
    "experiments": { "typedRoutes": true },
    "newArchEnabled": true,
    "extra": {
      "router": { "origin": false },
      "eas": { "projectId": "23b4e2dd-269e-4fdc-a235-8e0d9e28b886" }
    },
    "owner": "holidai"
  }
}
